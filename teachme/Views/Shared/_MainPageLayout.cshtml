@using teachme.Models.Home;
<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @RenderSection("head", required: false)
    <!--====== Required meta tags ======-->
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!--====== Title ======-->
    <title>TeachmeHow</title>

    <!--====== Favicon Icon ======-->
    @*@Styles.Render("~/Content/css")*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/MyMainLayOut.css" rel="stylesheet" />

    <link rel="shortcut icon" href="~/Content/images/favicon.png" type="image/png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

    <!--====== Slick css ======-->

    <link href="~/Content/css/slick.css" rel="stylesheet" />
    <!--====== Animate css ======-->

    <link href="~/Content/css/animate.css" rel="stylesheet" />
    <!--====== Nice Select css ======-->

    <link href="~/Content/css/nice-select.css" rel="stylesheet" />
    <!--====== Nice Number css ======-->

    <link href="~/Content/css/jquery.nice-number.min.css" rel="stylesheet" />
    <!--====== Magnific Popup css ======-->

    <link href="~/Content/css/magnific-popup.css" rel="stylesheet" />
    <!--====== Bootstrap css ======-->

    <link href="~/Content/css/MyMainLayOut.css" rel="stylesheet" />

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <!--====== Fontawesome css ======-->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <!--====== Default css ======-->

    <link href="~/Content/css/default.css" rel="stylesheet" />
    <!--====== Style css ======-->

    <link href="~/Content/css/style.css" rel="stylesheet" />
    <!--====== Responsive css ======-->
    <link href="~/Content/css/responsive.css" rel="stylesheet" />

</head>
<body>
    <!--====== PRELOADER PART START ======-->

    <div class="preloader">
        <div class="loader rubix-cube">
            <div class="layer layer-1"></div>
            <div class="layer layer-2"></div>
            <div class="layer layer-3 color-1"></div>
            <div class="layer layer-4"></div>
            <div class="layer layer-5"></div>
            <div class="layer layer-6"></div>
            <div class="layer layer-7"></div>
            <div class="layer layer-8"></div>
        </div>
    </div>

    <!--====== PRELOADER PART START ======-->
    <!--====== HEADER PART START ======-->

    <header id="header-part">

        <div class="header-top d-none d-lg-block">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="header-contact text-lg-left text-center">
                            <ul>
                                <li><img src="/Content/images/all-icon/map.png" alt="icon"><span>AHSANULLAH UNIVERSITY OF SCIENCE AND TECH</span></li>
                                <li><img src="/Content/images/all-icon/email.png" alt="icon"><span>info@TeachmeHow.com</span></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="header-opening-time text-lg-right text-center">
                            <p>"The beautiful thing about learning is that nobody can take it away from you"</p>
                        </div>
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- header top -->

        <div class="header-logo-support pt-30 pb-30">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-4">
                        <div class="logo">
                            <a href="index-2.html">
                                <img src="/Content/images/logo.png" alt="Logo">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-8 col-md-8">
                        <div class="support-button float-right d-none d-md-block">
                            <div class="support float-left">
                                <div class="icon">
                                    <img src="/Content/images/all-icon/support.png" alt="icon">
                                </div>
                                <div class="cont">
                                    <p>Need Help? call us free</p>
                                    <span>+8801955198774</span>
                                </div>
                            </div>
                            <div class="button float-left">
                                <a href="#" class="main-btn">Join Now</a>
                            </div>
                            <div class="button float-left">
                                <a href="#" class="main-btn">Sign UP</a>
                            </div>
                        </div>
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- header logo support -->

        <div class="navigation">
            <div class="container">
                <div class="row">
                    <div class="col-lg-10 col-md-10 col-sm-9 col-8">
                        <nav class="navbar navbar-expand-lg">
                            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                            <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                                <ul class="navbar-nav mr-auto">
                                    <li class="nav-item">
                                        @Html.ActionLink("Home", "Index", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("About", "About", "Home")
                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Courses", "Courses", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Events", "Eventy", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Instructor", "Instrcutor", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Blog", "Blog", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Shop", "Shop", "Home")

                                    </li>
                                    <li class="nav-item">
                                        @Html.ActionLink("Contact", "COntact", "Home")

                                    </li>
                                </ul>
                            </div>
                        </nav> <!-- nav -->
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-3 col-4">
                        <div class="right-icon text-right">
                            <ul>
                                <li><a href="#" id="search"><i class="fa fa-search"></i></a></li>
                                <ul class="nav navbar-nav navbar-right">
                                    <li class="dropdown">
                                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">Cart(0) <b class="caret"></b></a>
                                        <ul class="dropdown-menu">
                                           
                                            @if (Session["cart"] != null)
                                            {

                                                foreach (Item item in (List<Item>)Session["cart"])
                                                {
                                                    <li>
                                                        <a href="@Url.Action("RemoveFromCart", "Home", new { courseId=item.Courses.CourseId})">@item.Courses.CourseName <i class="fa fa-times"></i></a>
                                                    </li>
                                                }
                                            }
                                            <li class="divider"></li>
                                            <li><a href="@Url.Action("Checkout","Home")">Checkout</a></li>
                                            <li class="divider"></li>
                                        </ul>
                                    </li>
                                </ul>
                                
                            </ul>
                        </div> <!-- right icon -->
                    </div>
                </div> <!-- row -->
              
            </div> <!-- container -->
        </div>

    </header>

    <!--====== HEADER PART ENDS ======-->
    <div>
        @RenderBody()
    </div>

    <!--====== FOOTER PART START ======-->

    <footer id="footer-part">
        <div class="footer-top pt-40 pb-70">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-6">
                        <div class="footer-about mt-40">
                            <div class="logo">
                                <a href="#"><img src="/Content/images/logo.png" alt="Logo"></a>
                            </div>
                            <p>learn and prosper</p>
                            <ul class="mt-20">
                                <li><a href="#"><i class="fa fa-facebook-f"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                                <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                            </ul>
                        </div> <!-- footer about -->
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-6">
                        <div class="footer-link mt-40">
                            <div class="footer-title pb-25">
                                <h6>Sitemap</h6>
                            </div>
                            <ul>
                                <li><a href="/Content/index-2.html"><i class="fa fa-angle-right"></i>Home</a></li>
                                <li><a href="/Content/about.html"><i class="fa fa-angle-right"></i>About us</a></li>
                                <li><a href="/Content/courses.html"><i class="fa fa-angle-right"></i>Courses</a></li>
                                <li><a href="/Content/blog.html"><i class="fa fa-angle-right"></i>News</a></li>
                                <li><a href="/Content/events.html"><i class="fa fa-angle-right"></i>Event</a></li>
                            </ul>
                            <ul>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Gallery</a></li>
                                <li><a href="/Content/shop.html"><i class="fa fa-angle-right"></i>Shop</a></li>
                                <li><a href="/Content/teachers.html"><i class="fa fa-angle-right"></i>Teachers</a></li>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Support</a></li>
                                <li><a href="/Content/contact.html"><i class="fa fa-angle-right"></i>Contact</a></li>
                            </ul>
                        </div> <!-- footer link -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-sm-6">
                        <div class="footer-link support mt-40">
                            <div class="footer-title pb-25">
                                <h6>Support</h6>
                            </div>
                            <ul>
                                <li><a href="#"><i class="fa fa-angle-right"></i>FAQS</a></li>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Privacy</a></li>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Policy</a></li>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Support</a></li>
                                <li><a href="#"><i class="fa fa-angle-right"></i>Documentation</a></li>
                            </ul>
                        </div> <!-- support -->
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="footer-address mt-40">
                            <div class="footer-title pb-25">
                                <h6>Contact Us</h6>
                            </div>
                            <ul>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-home"></i>
                                    </div>
                                    <div class="cont">
                                        <p>AHSANULLAH UNIVERSITY OF SCIENCE AND TECHNOLOGY</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-phone"></i>
                                    </div>
                                    <div class="cont">
                                        <p>+8801955198774</p>
                                    </div>
                                </li>
                                <li>
                                    <div class="icon">
                                        <i class="fa fa-envelope-o"></i>
                                    </div>
                                    <div class="cont">
                                        <p>info@teachemehow.com</p>
                                    </div>
                                </li>
                            </ul>
                        </div> <!-- footer address -->
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- footer top -->

        <div class="footer-copyright pt-10 pb-25">
            <div class="container">
                <div class="row">
                    <div class="col-md-8">
                        <div class="copyright text-md-left text-center pt-15">
                            <p><a target="_blank" href="#">TeachmeHOW</a> </p>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="copyright text-md-right text-center pt-15">

                        </div>
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- footer copyright -->
    </footer>

    <!--====== FOOTER PART ENDS ======-->
    <!--====== BACK TO TP PART START ======-->

    <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>

    <!--====== BACK TO ToP PART ENDS ======-->
    <!--====== jquery js ======-->
    <script src="~/Content/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="~/Content/js/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!--====== Slick js ======-->
    <script src="~/Content/js/slick.min.js"></script>

    <!--====== Magnific Popup js ======-->
    <script src="~/Content/js/jquery.magnific-popup.min.js"></script>

    <!--====== Counter Up js ======-->
    <script src="~/Content/js/waypoints.min.js"></script>
    <script src="~/Content/js/jquery.counterup.min.js"></script>
    <!--====== Nice Select js ======-->
    <script src="~/Content/js/jquery.nice-select.min.js"></script>

    <!--====== Nice Number js ======-->
    <script src="~/Content/js/jquery.nice-number.min.js"></script>

    <!--====== Count Down js ======-->
    <script src="~/Content/js/jquery.countdown.min.js"></script>

    <!--====== Validator js ======-->
    <script src="~/Content/js/validator.min.js"></script>

    <!--====== Ajax Contact js ======-->
    <script src="~/Content/js/ajax-contact.js"></script>

    <!--====== Main js ======-->
    <script src="~/Content/js/main.js"></script>

    <!--====== Map js ======-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDC3Ip9iVC0nIxC6V14CKLQ1HZNF_65qEQ"></script>
    <script src="~/Content/js/map-script.js"></script>


</body>
</html>
